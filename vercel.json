{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/dashboard"
    },
    {
      "src": "/dashboard",
      "dest": "/static/dashboard.html"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.py"
    }
  ]
}
